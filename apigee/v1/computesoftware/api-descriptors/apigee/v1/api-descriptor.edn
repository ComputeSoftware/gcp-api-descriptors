 "apigee.organizations.environments.optimizedStats.get" #:compute.gcp.descriptor{:http-method :get, :path "v1/organizations/{organizationsId}/environments/{environmentsId}/optimizedStats/{optimizedStatsId}", :response {"$ref" "GoogleCloudApigeeV1OptimizedStats"}, :parameters {"topk" {"description" "Take 'top k' results from results, for example, to return the top 5\nresults 'topk=5'.", "type" "string", "location" "query"}, "timeUnit" {"description" "A value of second, minute, hour, day, week, month.\nTime Unit specifies the granularity of metrics returned.", "type" "string", "location" "query"}, "realtime" {"location" "query", "description" "Legacy field: not used anymore", "type" "boolean"}, "offset" {"description" "Use offset with limit to enable pagination of results. For example,\nto display results 11-20, set limit to '10' and offset to '10'.", "type" "string", "location" "query"}, "tsAscending" {"type" "boolean", "location" "query", "description" "Lists timestamps in ascending order if set to true. Recommend setting\nthis value to true if you are using sortby with sort=DESC."}, "sortby" {"description" "Comma separated list of columns to sort the final result.", "type" "string", "location" "query"}, "sort" {"type" "string", "location" "query", "description" "This parameter specifies if the sort order should be ascending or\ndescending Supported values are DESC and ASC."}, "name" {"location" "path", "description" "Required. The organization and environment name for which the interactive\nquery will be executed. Must be of the form\n  `organizations/{organization_id}/environments/{environment_id/stats/{dimensions}`\nDimensions let you view metrics in meaningful groupings. E.g. apiproxy,\ntarget_host. The value of dimensions should be comma separated list as\nshown below\n`organizations/{org}/environments/{env}/stats/apiproxy,request_verb`", "required" true, "type" "string", "pattern" "^organizations/[^/]+/environments/[^/]+/optimizedStats/.*$"}, "limit" {"descrip