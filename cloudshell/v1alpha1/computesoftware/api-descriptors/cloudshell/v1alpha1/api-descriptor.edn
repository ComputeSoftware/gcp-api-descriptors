vironments.publicKeys.delete" #:compute.gcp.descriptor{:http-method :delete, :path "v1alpha1/{+name}", :response {"$ref" "Empty"}, :parameters {"name" {"location" "path", "type" "string", "required" true, "pattern" "^users/[^/]+/environments/[^/]+/publicKeys/[^/]+$", "description" "Name of the resource to be deleted, e.g. `users/me/environments/default/publicKeys/my-key`."}}, :description "Removes a public SSH key from an environment. Clients will no longer be able to connect to the environment using the corresponding private key."}, "cloudshell.users.environments.publicKeys.create" #:compute.gcp.descriptor{:http-method :post, :path "v1alpha1/{+parent}/publicKeys", :request {"$ref" "CreatePublicKeyRequest"}, :response {"$ref" "PublicKey"}, :parameters {"parent" {"description" "Parent resource name, e.g. `users/me/environments/default`.", "pattern" "^users/[^/]+/environments/[^/]+$", "location" "path", "type" "string", "required" true}}, :description "Adds a public SSH key to an environment, allowing clients with the corresponding private key to connect to that environment via SSH. If a key with the same format and content already exists, this will return the existing key."}, "cloudshell.users.environments.authorize" #:compute.gcp.descriptor{:http-method :post, :path "v1alpha1/{+name}:authorize", :request {"$ref" "AuthorizeEnvironmentRequest"}, :response {"$ref" "Empty"}, :parameters {"name" {"required" true, "description" "Name of the resource that should receive the credentials, for example `users/me/environments/default` or `users/someone@example.com/environments/default`.", "location" "path", "type" "string", "pattern" "^users/[^/]+/environments/[^/]+$"}}, :description "Sends OAuth credentials to a running environment on behalf of a user. When this completes, the environment will be authorized to run various Google Cloud command line tools without requiring the user to manually authenticate."}, "cloudshell.users.environments.start" #:compute.gcp.descriptor{:http-